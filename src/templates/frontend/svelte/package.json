{
  "name": "{{#if (eq projectType 'monorepo')}}frontend{{else}}{{projectName}}{{/if}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview"{{#if frontend.eslint}},
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --fix"{{/if}}{{#if frontend.prettier}},
    "format": "prettier --write ."{{/if}}
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.1",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^17.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.19.2",
    "svelte-check": "^4.1.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.11"{{#if (eq frontend.uiComponents 'shadcn-svelte')}},
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.564.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"{{/if}}{{#if (eq frontend.uiComponents 'smui')}},
    "@smui/button": "^8.0.0-beta.14",
    "@smui/card": "^8.0.0-beta.14",
    "@smui/textfield": "^8.0.0-beta.14",
    "svelte-material-ui": "^8.0.0-beta.14"{{/if}}
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/kit": "^2.16.1"{{#if (eq frontend.uiComponents 'headless-ui')}},
    "@headlessui/svelte": "^1.0.0"{{/if}}{{#if (eq frontend.uiComponents 'agnostic-ui')}},
    "agnostic-ui": "^3.2.1"{{/if}}
  }
}
